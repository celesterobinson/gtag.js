{
  "displayName": "gtag.js",
  "name": "gtag.js",
  "platform": "web",
  "version": "1.0.0",
  "description": "Load Google's gtag.js into your site to send event data to Google Analytics, Google Ads, and Google Marketing Platform.",
  "iconPath": "google-g.svg",
  "author": {
    "name": "Celeste Robinson"
  },
  "viewBasePath": "src/view/",
  "configuration": {
    "viewPath": "configuration/configuration.html",
    "schema": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "properties": {
        "conversionIds": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  },
  "main": "src/lib/gtagCode.js"
}
